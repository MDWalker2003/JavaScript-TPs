{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "-webkit-text-size-adjust",
          "-webkit-text-size-adjust",
          "-webkit-tap-highlight-color",
          "-webkit-tap-highlight-color",
          "-webkit-tap-highlight-color",
          "-webkit-tap-highlight-color",
          "-webkit-tap-highlight-color",
          "-webkit-tap-highlight-color",
          "-webkit-tap-highlight-color",
          "-webkit-tap-highlight-color",
          "text-decoration-skip-ink",
          "text-decoration-skip-ink",
          "-webkit-tap-highlight-color",
          "-webkit-tap-highlight-color"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "button-type": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 113",
    "not firefox <= 113",
    "not safari <= 16.5",
    "not and_ff <= 113"
  ]
}